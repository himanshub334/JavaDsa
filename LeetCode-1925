class Solution {
    public int countTriples(int n) {
        int count=0;
        for(int i=1;i<n;i++){
            for(int j=i+1;j<n;j++){
                int sumSquare=i*i+j*j;
                int k=(int) Math.sqrt(sumSquare);
                if(k*k==sumSquare && k<=n){
                    count+=2;
                }
            }
        }
        return count;
    }
}
